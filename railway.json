{
    "build": {
        "builder": "DOCKERFILE",
        "dockerfilePath": "backend/Dockerfile"
    },
    "deploy": {
        "numReplicas":1,
        "startCommand": "uvicorn ap.main:app --host 0.0.0.0 --port $PORT",
        "restartPolicyType": "ON_FAILURE",
        "restartPolicyMaxRetries": 10
    }
}